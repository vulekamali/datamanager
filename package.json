    "pym.js": "^1.3.2",
    "query-string": "^5.1.1",
    "react-ga": "^2.5.3",
    "react-html-connector": "^0.2.6",
    "react-scripts": "^5.0.0",
    "redux": "^4.0.1",
    "save-svg-as-png": "^1.4.6",
    "url-search-params-polyfill": "^8.1.0",
    "uuid": "^3.3.2",
    "vulekamali-visualisations": "https://github.com/vulekamali/data-visualisations.git#2e861cce3a27bd1b6791bdd9f4fd1c4497bed1b7",
    "whatwg-fetch": "^2.0.3",
    "yarn": "^1.13.0"
  },
  "importWebflowExport": {
    "copyTrees": {
      "css": "budgetportal/webflow/static/css",
      "js": "budgetportal/webflow/static/js",
      "images": "budgetportal/webflow/static/images",
      "fonts": "budgetportal/webflow/static/fonts"
    },
    "importHtml": [
      {
        "glob": "*.html",
        "destDir": "budgetportal/webflow/templates/webflow",
        "transforms": "./assets/js/webflow/import.js"
      }
    ]
  }
}
