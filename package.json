    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-app": "^5.0.1",
    "front-matter": "^2.3.0",
    "frontmatter": "0.0.3",
    "fs-extra": "^7.0.1",
    "highlightjs": "^9.12.0",
    "html": "^1.0.0",
    "html-entities": "^1.2.1",
    "html-loader": "^0.5.5",
    "import-webflow-export": "^0.0.3",
    "jest": "^24.8.0",
    "markdown-it": "^8.4.2",
