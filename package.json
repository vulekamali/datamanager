    "serve-static": "^1.13.2",
    "style-loader": "^0.18.2",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
